{
  "name": "electron-playground-ts",
  "version": "1.0.0",
  "description": "A ts playground in electron",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "clean": "npm run -s clean:dist",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js"
  },
  "repository": "https://github.com/schamane/electron-playground-ts",
  "author": "GitHub",
  "license": "MIT",
  "keywords": ["Electron", "playground", "typescript"],
  "devDependencies": {
    "@types/lodash": "^4.14.109",
    "@types/pixi.js": "^4.7.3",
    "@types/source-map-support": "^0.4.0",
    "electron": "^2.0.1",
    "electron-reload": "^1.2.2",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "fontfaceobserver": "^2.0.13",
    "lodash": "^4.17.10",
    "pixi-filters": "^2.6.1",
    "pixi.js": "^4.7.3",
    "source-map-support": "^0.5.6"
  }
}
